{
  "name": "dwnload/wp-settings-api",
  "description": "A PHP class abstraction that removes all the headaches of the WordPress settings API under the hood and builds a nice options panel on the fly.",
  "version": "2.1.5",
  "license": "MIT",
  "authors": [
    {
      "name": "Austin Passy",
      "email": "thefrosty@users.noreply.github.com",
      "homepage": "https://austin.passy.co",
      "role": "Developer"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "thefrosty/wp-utilities": "^0.2.2",
    "php": ">=7.0.4"
  },
  "require-dev": {
    "10up/wp_mock": "dev-dev",
    "phpunit/phpunit": "5.7.*",
    "wp-coding-standards/wpcs": "^0.13.1"
  },
  "scripts": {
    "post-install-cmd": [
      "@phpcs"
    ],
    "post-update-cmd": [
      "@phpcs"
    ],
    "phpcs": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs"
  },
  "autoload": {
    "psr-4": {
      "Dwnload\\WpSettingsApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dwnload\\WpSettingsApi\\Tests\\": "tests"
    }
  }
}
